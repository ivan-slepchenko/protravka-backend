{
  "name": "protravka-backend",
  "version": "1.0.0",
  "description": "Backend for Protravka project",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "migration:create": "typeorm migration:create src/migrations -n",
    "migration:generate": "npm run build && typeorm migration:generate -d dist/database.js src/migrations/auto",
    "migration:run": "npm run build && typeorm -- migration:run -d dist/database.js",
    "migration:revert": "npm run build && typeorm migration:revert -d dist/database.js"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.1",
    "@types/sequelize": "^4.28.20",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.4",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "typeorm": "^0.3.20"
  }
}
